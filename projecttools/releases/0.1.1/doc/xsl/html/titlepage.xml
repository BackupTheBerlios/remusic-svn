<t:templates xmlns:t="http://nwalsh.com/docbook/xsl/template/1.0"
             xmlns:param="http://nwalsh.com/docbook/xsl/template/1.0/param"
             xmlns:xsl="http://www.w3.org/1999/XSL/Transform">

  <t:titlepage t:element="book" t:wrapper="div" t:class="titlepage">
    <t:titlepage-content t:side="recto">
      <title/>
      <subtitle/>
      <authorgroup/>
      <author/>
      <publisher/>
      <copyright/>
      <legalnotice/>
      <revhistory/>
      <revision/>
      <releaseinfo/>
    </t:titlepage-content>
    
    <t:titlepage-content t:side="verso">
    </t:titlepage-content>
    
    <t:titlepage-separator>
    </t:titlepage-separator>
    
    <t:titlepage-before t:side="recto">
    </t:titlepage-before>
    
    <!--
      <t:titlepage-before t:side="verso">
      <img align ="left" width="100" src="figures/remus-logo.png" alt="Remus Logo"/>
      </t:titlepage-before>
    -->
    
  </t:titlepage>
  
  <t:titlepage t:element="article" t:wrapper="div" t:class="titlepage">
    <t:titlepage-content t:side="recto">
      <title/>
      <subtitle/>
      <authorgroup/>
      <author/>
      <publisher/>
      <copyright/>
      <legalnotice/>
      <revhistory/>
      <revision/>
      <releaseinfo/>
    </t:titlepage-content>
    
    <t:titlepage-content t:side="verso">
    </t:titlepage-content>
    
    <t:titlepage-separator>
    </t:titlepage-separator>
    
    <t:titlepage-before t:side="recto">
    </t:titlepage-before>
    
    <!--
      <t:titlepage-before t:side="verso">
      <img align ="left" width="100" src="figures/remus-logo.png" alt="Remus Logo"/>
      </t:titlepage-before>
    -->
    
  </t:titlepage>
</t:templates>
